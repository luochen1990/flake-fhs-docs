{
  "name": "flake-fhs-docs",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check && tsc --noEmit && npm run check:links",
    "lint:links:internal-refs": "find src/content/docs -name '*.md*' -exec grep -H '\\.md' {} \\;",
    "check:links:external": "lychee --exclude-scheme file src/content/docs",
    "check:links": "pnpm run build && lychee --config lychee.toml dist",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.0",
    "@astrojs/react": "^4.4.2",
    "@astrojs/starlight": "^0.26.0",
    "@astrojs/tailwind": "^6.0.2",
    "@tailwindcss/typography": "^0.5.19",
    "astro": "^4.14.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
